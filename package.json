{
  "name": "baedal",
  "version": "0.0.0",
  "description": "A TypeScript-based CLI tool and npm library for downloading files/folders from Git repositories",
  "keywords": [
    "git",
    "download",
    "cli",
    "github",
    "gitlab",
    "bitbucket",
    "degit",
    "repository"
  ],
  "license": "MIT",
  "author": "KubrickCode",
  "repository": {
    "type": "git",
    "url": "https://github.com/KubrickCode/baedal.git"
  },
  "bugs": {
    "url": "https://github.com/KubrickCode/baedal/issues"
  },
  "homepage": "https://github.com/KubrickCode/baedal#readme",
  "type": "module",
  "bin": {
    "baedal": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "globby": "^14.0.0",
    "ofetch": "^1.3.0",
    "picocolors": "^1.0.0",
    "tar": "^7.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/tar": "^6.1.13",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
